+int delSchar(char *buf, char *a)
+{
+    int n=0, k=0;
+    for(int i=0; buf[i]!='\0'; i++)
+    {
+       if(buf[i]==a[n])
+       {
+           k = i;
+           while(buf[i]==a[n])
+           {
+               if(a[++n]=='\0')
+               {
+                   for(k; buf[k+n]!='\0'; k++)
+                   {
+                       buf[k] = buf[k+n];
+                   }
+                   buf[k]='\0';
+                   return 1;
+               }
+               i++;
+           }
+           n=0;
+           i--;
+       }
+    }
+    return 0;
+}
+
+void handle_parentheses(char* input)
+{
+    char buf[1024];
+    int len = 0;
+    for(int i=0; input[i] != '\0' ; i++){
+        if(input[i] == ' ')
+        {
+            input[i] = '\\';
+            len = strlen(input);
+            strncpy(buf, input+i+1, len-i);
+            input[i+1] = ' ';
+            input[i+2] = '\0';
+            i++;
+            strcat(input, buf);
+        }
+        if(input[i] == '(')
+        {
+            input[i] = '\\';
+            len = strlen(input);
+            strncpy(buf, input+i+1, len-i);
+            input[i+1] = '(';
+            input[i+2] = '\0';
+            i++;
+            strcat(input, buf);
+        }
+        if(input[i] == ')')
+        {
+            input[i] = '\\';
+            len = strlen(input);
+            strncpy(buf, input+i+1, len-i);
+            input[i+1] = ')';
+            input[i+2] = '\0';
+            i++;
+            strcat(input, buf);
+        }
+    }
+}
 
